{
  "name": "@wesbitty/wesjet",
  "description": "Incremental Bundler, Transform & Validate content and build system optimized for JavaScript and TypeScript",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "scripts": {
    "postinstall": "ts-patch install && ts-patch --persist && npm run postinstall -w wesjet",
    "prepack": "npm run prepack -w next-config-wesjet",
    "lint": "npm run format:prettier && npm run format:eslint",
    "build": "tsc -b tsconfig.build.json",
    "rm": "bash -c 'rm -rf packages/@wesjet/*/dist packages/*/dist packages/*/.wesjet'",
    "check:prettier": "prettier -c packages",
    "check:eslint": "eslint packages --ext .ts --max-warnings=0",
    "format:prettier": "prettier -w packages",
    "format:eslint": "eslint packages --ext .ts --fix",
    "all": "npm i && npm run lint && npm run build && npm -w next-config-wesjet run prepack"
  },
  "author": "Wesbitty, Inc",
  "keywords": [
    "Wesjet",
    "Bundler",
    "Typescript",
    "Maker"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbitty/wesjet"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@effect-ts/tracing-plugin": "^0.20.0",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.8.3",
    "ts-patch": "^2.1.0",
    "typescript": "^4.9.4"
  },
  "workspaces": [
    "packages/@wesjet/*",
    "packages/*",
    "tests"
  ],
  "resolutions": {
    "esbuild": "0.17.0",
    "wesjet": "*",
    "@wesjet/*": "*",
    "stackbit-config-wesjet": "*",
    "next-config-wesjet": "*",
    "rxjs": "^7.1.0"
  }
}
