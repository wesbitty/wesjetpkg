{
  "name": "@wesbitty/wesjet",
  "description": "Incremental Bundler, Transform & Validate content and build system optimized for JavaScript and TypeScript",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "scripts": {
    "postinstall": "ts-patch install -s",
    "prepare": "husky install",
    "build": "rimraf packages/*/dist packages/@wesjet/*/dist packages/*/.wesjet && tsc -b tsconfig.build.json",
    "pre-commit": "npx lint-staged",
    "check-prettier": "prettier -c packages",
    "check-eslint": "eslint packages --ext .ts --max-warnings=0",
    "format-prettier": "prettier -w packages",
    "eslint-fix": "eslint packages --ext .ts --fix"
  },
  "author": "Wesbitty, Inc",
  "keywords": [
    "Wesjet",
    "Bundler",
    "Typescript",
    "Maker"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbitty/wesjet"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@effect-ts/tracing-plugin": "^0.20.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.46",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "browserslist": "^4.21.5",
    "caniuse-lite": "^1.0.30001476",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-wesjet": "^0.0.20",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.3",
    "swr": "^2.1.2",
    "ts-patch": "^2.1.0",
    "typescript": "^4.9.4"
  },
  "workspaces": [
    "packages/@wesjet/*",
    "packages/*",
    "tests"
  ],
  "resolutions": {
    "esbuild": "0.17.0",
    "wesjet": "*",
    "@wesjet/*": "*",
    "stackbit-config-wesjet": "*",
    "next-config-wesjet": "*",
    "rxjs": "^7.1.0"
  }
}
