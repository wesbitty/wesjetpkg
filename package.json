{
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "wesjet",
    "bundler",
    "javascript",
    "typescript",
    "transform",
    "validate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wesbitty/wesjet.git"
  },
  "license": "MIT",
  "author": "Wesbitty, Inc",
  "workspaces": [
    "packages/@wesjet/*",
    "packages/*",
    "tests"
  ],
  "scripts": {
    "postinstall": "ts-patch install -s",
    "prepare": "husky install",
    "build": "npm run clean && tsc -b tsconfig.build.json",
    "postbuild": "npm run build --workspaces --if-present",
    "check-prettier": "prettier --check .",
    "check-eslint": "eslint . --ext .ts --max-warnings=0",
    "prettier": "prettier --write .",
    "lint": "eslint . --ext .ts --fix",
    "lint:package": "sort-package-json",
    "clean": "bash -c 'rm -rf packages/*/lib packages/@wesjet/*/lib packages/*/.wesjet'",
    "test": "CI=true VITEST_SEGFAULT_RETRY=3 npm run test --workspaces --parallel"
  },
  "lint-staged": {
    "package.json": [
      "npm run lint:package"
    ],
    "*.{mts,cts,ts,tsx,yml,js,jsx,mjs,cjs,json,md,mdx,css}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "esbuild": "0.17.0",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@changesets/cli": "^2.26.1",
    "@effect-ts/tracing-plugin": "^0.20.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.46",
    "@types/jest": "^29.5.1",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "browserslist": "^4.21.5",
    "caniuse-lite": "^1.0.30001476",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-wesjet": "^0.0.37",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "genversion": "^3.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "nanoid": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "sort-package-json": "^2.4.1",
    "swr": "^2.1.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "typedoc": "^0.24.4",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  }
}
