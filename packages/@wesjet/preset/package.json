{
  "name": "@wesjet/preset",
  "version": "0.0.34",
  "description": "wesjet package to preset layout's",
  "keywords": [
    "wesjet"
  ],
  "homepage": "https://wesbitty.org/product/wesjet",
  "bugs": "https://github.com/wesbitty/wesjet/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbitty/wesjet.git",
    "directory": "packages/@wesjet/preset"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    },
    "./schema/defs": {
      "import": "./lib/schema/defs/index.js"
    }
  },
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "schema": [
        "./lib/schema"
      ]
    }
  },
  "files": [
    "lib",
    "./lib/!(__test__)/**/*.{js,ts,map}"
  ],
  "scripts": {
    "clean": "rimraf ./lib/",
    "test": "TZ='GMT' FORCE_COLOR=1 vitest"
  },
  "dependencies": {
    "@wesjet/core": "^0.0.34",
    "@wesjet/function.js": "^0.0.34",
    "chokidar": "^3.5.3",
    "fast-glob": "^3.2.12",
    "gray-matter": "^4.0.3",
    "imagescript": "^1.2.15",
    "micromatch": "^4.0.5",
    "ts-pattern": "^4.1.3",
    "unified": "^10.1.2",
    "yaml": "^1.10.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/faker": "^5.5.8",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.31.1",
    "@types/yaml": "^1.9.7",
    "faker": "^5.5.3",
    "sharp": "^0.31.3",
    "vite": "^3.2.7",
    "vitest": "0.27.1"
  }
}
