{
  "name": "eslint-plugin-wesjet",
  "version": "0.0.20",
  "description": "Wesjet Linting and Formatting plugin",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "DimejiSR",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "swc lib -d dist",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:ts": "eslint .",
    "test": "mocha tests --recursive",
    "gen-doc": "eslint-doc-generator"
  },
  "dependencies": {
    "glob": "^10.0.0",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "eslint": "^7.24.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5"
  }
}
