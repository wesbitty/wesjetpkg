{
  "name": "eslint-plugin-wesjet",
  "version": "0.0.34",
  "bin": "./bin/generator.ts",
  "description": "ESLint configuration used by Wesjet.",
  "main": "./src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "repository": {
    "url": "wesbitty/wesjet",
    "directory": "packages/eslint-config-wesjet"
  },
  "keywords": [
    "wesjet",
    "eslint"
  ],
  "files": [
    "src",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "swc src -d lib --config-file config/.swcrc"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@typescript-eslint/parser": "^5.42.0",
    "@typescript-eslint/utils": "^5.59.0",
    "chalk": "^5.2.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-wesjet": "^0.0.33",
    "glob": "7.1.7"
  },
  "devDependencies": {
    "@types/glob": "7.1.1",
    "eslint": "7.24.0"
  },
  "peerDependencies": {
    "eslint": "^7.23.0 || ^8.0.0",
    "typescript": ">=3.3.1"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
