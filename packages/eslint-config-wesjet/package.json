{
  "name": "eslint-config-wesjet",
  "description": "wesjet eslint plugin for code scanning",
  "version": "0.0.30",
  "license": "MIT",
  "keywords": [
    "eslint",
    "wesjet"
  ],
  "files": [
    "index.js"
  ],
  "author": "dimejiSR <Wesbitty Inc>",
  "homepage": "https://wesbitty.org/product/wesjet",
  "bugs": "https://github.com/wesbitty/wesjet/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbitty/wesjet.git",
    "directory": "packages/eslint-plugin-wesjet"
  },
  "main": "index.js",
  "scripts": {
    "build": "swc src -d lib",
    "tsc": "tsc",
    "lint": "npm-run-all \"lint:*\"",
