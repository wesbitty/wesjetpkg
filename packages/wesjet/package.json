{
  "name": "wesjet",
  "description": "Wesjet is a JavaScript library to transform and validate content for building user interfaces.",
  "version": "0.0.30",
  "license": "MIT",
  "engines": {
    "node": ">=14.18"
  },
  "homepage": "https://wesbitty.org/product/wesjet",
  "bugs": "https://github.com/wesbitty/wesjet/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbitty/wesjet.git",
    "directory": "packages/wesjet"
  },
  "keywords": [
    "wesjet"
  ],
  "files": [
    "./bin/install-native.cjs",
    "./lib/**/*.{cjs,js,ts,map}",
    "./lib/!(__test__)/**/*.{js,ts,map}",
    "index.d.ts",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "bin": "./bin/install-native.cjs",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "import": "./lib/client/index.js"
    },
    "./core": {
      "import": "./lib/core.js"
    },
    "./function.js": {
      "import": "./lib/function.js/index.js"
    },
    "./function.js/node": {
      "import": "./lib/function.js/node/index.js"
    },
    "./next": {
      "import": "./lib/shared/next/index.js",
      "require": "./lib/shared/next/index-cjs.cjs"
    },
    "./next/hook": {
      "import": "./lib/shared/next/hook/index.js",
      "require": "./lib/shared/next/hook/index.js"
    },
    "./preset": {
      "import": "./lib/preset/index.js"
    },
    "./preset/schema": {
      "import": "./lib/preset/schema/index.js"
    },
    "./sanity": {
      "import": "./lib/shared/sanity/index.js"
    },
    "./contentful": {
      "import": "./lib/shared/contentful/index.js"
    },
    "./stackbit": {
      "import": "./lib/shared/stackbit/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "./package.json"
      ],
      "client": [
        "./lib/client"
      ],
      "core": [
        "./lib/core"
      ],
      "function.js": [
        "./lib/function.js"
      ],
      "function.js/node": [
        "./lib/function.js/node"
      ],
      "next": [
        "./lib/shared/next"
      ],
      "next/hook": [
        "./lib/shared/next/hook"
      ],
      "preset": [
        "./lib/preset"
      ],
      "preset/schema": [
        "./lib/preset/schema"
      ],
      "contentful": [
        "./lib/shared/contentful"
      ],
      "stackbit": [
        "./lib/shared/stackbit"
      ],
      "sanity": [
        "./lib/shared/sanity"
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./bin/install-native.cjs postinstall",
    "prepack": "sed '/^import\\|^export/d' lib/shared/next/index-cjs.js > lib/shared/next/index-cjs.cjs; cp src/shared/next/hook/ReactJSX.cjs lib/shared/next/hook/ReactJSX.cjs",
    "clean": "rimraf lib .wesjet"
  },
  "peerDependencies": {
    "@stackbit/sdk": "0.3.x",
    "next": "^12 || ^13",
    "react": "*",
    "react-dom": "*"
  },
  "peerDependenciesMeta": {
    "@stackbit/sdk": {
      "optional": true
    }
  },
  "dependencies": {
    "@sanity/client": "^2.10.5",
    "@sanity/core": "^2.10.5",
    "@sanity/image-url": "^0.140.22",
    "@sanity/schema": "^2.10.0",
    "@stackbit/sdk": "0.3.x",
    "@wesjet/core": "^0.0.30",
    "@wesjet/function.js": "^0.0.30",
    "@wesjet/preset": "^0.0.30",
    "clipanion": "^3.2.0-rc.14",
    "contentful-management": "^7.22.4",
    "cross-fetch": "^3.1.5",
    "rxjs": "^7.1.0",
    "typanion": "^3.12.1"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "jsonwebtoken": "^9.0.0",
    "next": "^13.1.2",
    "typedoc": "^0.24.4",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "0.29.7"
  }
}
