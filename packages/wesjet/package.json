{
  "name": "wesjet",
  "description": "Wesjet is a JavaScript library to transform and validate content for building user interfaces.",
  "version": "0.0.21",
  "license": "MIT",
  "keywords": [
    "wesjet"
  ],
  "bin": "./bin/install-cmd.cjs",
  "type": "module",
  "engines": {
    "node": ">=14.18"
  },
  "homepage": "https://wesbitty.org/product/wesjet",
  "bugs": "https://github.com/wesbitty/wesjet/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbitty/wesjet.git",
    "directory": "packages/wesjet"
  },
  "files": [
    "bin",
    "dist",
    "core.js",
    "core.d.ts",
    "preset.js",
    "preset.d.ts",
    "client.js",
    "client.d.ts",
    "index.d.ts",
    "next.d.ts",
    "next.js",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "import": "./dist/client/index.js"
    },
    "./core": {
      "import": "./dist/core.js"
    },
    "./function": {
      "import": "./dist/function/index.js"
    },
    "./preset": {
      "import": "./dist/preset.js"
    },
    "./next": {
      "import": "./dist/next/index.js",
      "require": "./dist/next/_jsx_.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "./package.json"
      ],
      "client": [
        "./dist/client"
      ],
      "core": [
        "./dist/core"
      ],
      "function": [
        "./dist/function"
      ],
      "preset": [
        "./dist/preset"
      ],
      "next": [
        "./dist/next"
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./bin/install-cmd.cjs postinstall",
    "prepack": "sed '/^import /d' dist/next/_jsx_.js > dist/next/_jsx_.cjs; cp src/next/lib/jsx-runtime.cjs dist/next/lib/jsx-runtime.cjs"
  },
  "peerDependencies": {
    "@stackbit/sdk": "^0.2.39",
    "next": "^12 || ^13",
    "react": "*",
    "react-dom": "*"
  },
  "peerDependenciesMeta": {
    "@stackbit/sdk": {
      "optional": true
    }
  },
  "dependencies": {
    "@sanity/client": "^2.10.5",
    "@sanity/core": "^2.10.5",
    "@sanity/image-url": "^0.140.22",
    "@sanity/schema": "^2.10.0",
    "@stackbit/sdk": "^0.2.39",
    "@wesjet/core": "^0.0.21",
    "@wesjet/preset": "^0.0.21",
    "@wesjet/function.js": "^0.0.21",
    "clipanion": "^3.2.0-rc.14",
    "contentful-management": "^7.22.4",
    "rxjs": "^7.1.0",
    "typanion": "^3.12.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "next": "^13.1.2",
    "typescript": "^4.9.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.1"
  }
}
