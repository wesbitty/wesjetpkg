{
  "name": "wesjet",
  "description": "Wesjet is a JavaScript library to transform and validate content for building user interfaces.",
  "version": "0.0.22-automated.8",
  "license": "MIT",
  "engines": {
    "node": ">=14.18"
  },
  "homepage": "https://wesbitty.org/product/wesjet",
  "bugs": "https://github.com/wesbitty/wesjet/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbitty/wesjet.git",
    "directory": "packages/wesjet"
  },
  "keywords": [
    "wesjet"
  ],
  "files": [
    "bin",
    "core.js",
    "core.d.ts",
    "client.js",
    "client.d.ts",
    "dist",
    "index.d.ts",
    "next",
    "preset",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "sideEffects": false,
  "bin": "./bin/install-native.cjs",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "import": "./dist/client/index.js"
    },
    "./core": {
      "import": "./dist/core.js"
    },
    "./function.js": {
      "import": "./dist/function.js/index.js"
    },
    "./function.js/node": {
      "import": "./dist/function.js/node/index.js"
    },
    "./preset": {
      "import": "./dist/preset/index.js"
    },
    "./preset/schema": {
      "import": "./dist/preset/schema/index.js"
    },
    "./next": {
      "import": "./dist/next/index.js",
      "require": "./dist/next/index-cjs.cjs"
    },
    "./next/hook": {
      "import": "./dist/next/hook/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "./package.json"
      ],
      "client": [
        "./dist/client"
      ],
      "core": [
        "./dist/core"
      ],
      "function.js": [
        "./dist/function.js"
      ],
      "function.js/node": [
        "./dist/function.js/node"
      ],
      "preset": [
        "./dist/preset"
      ],
      "preset/schema": [
        "./dist/preset/schema"
      ],
      "next": [
        "./dist/next"
      ],
      "next/hook": [
        "./dist/next/hook"
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./bin/install-native.cjs postinstall",
    "build": "sed '/^import /d' dist/next/index-cjs.js > dist/next/index-cjs.cjs; cp src/next/hook/ReactJSX.cjs dist/next/hook/ReactJSX.cjs"
  },
  "peerDependencies": {
    "@stackbit/sdk": "^0.2.39",
    "next": "^12 || ^13",
    "react": "*",
    "react-dom": "*"
  },
  "peerDependenciesMeta": {
    "@stackbit/sdk": {
      "optional": true
    }
  },
  "dependencies": {
    "@sanity/client": "^2.10.5",
    "@sanity/core": "^2.10.5",
    "@sanity/image-url": "^0.140.22",
    "@sanity/schema": "^2.10.0",
    "@stackbit/sdk": "^0.2.39",
    "@wesjet/core": "^0.0.22-automated.8",
    "@wesjet/preset": "^0.0.22-automated.8",
    "@wesjet/function.js": "^0.0.22-automated.8",
    "clipanion": "^3.2.0-rc.14",
    "contentful-management": "^7.22.4",
    "rxjs": "^7.1.0",
    "typanion": "^3.12.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "next": "^13.1.2",
    "typescript": "^4.9.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.1"
  }
}
