{
  "name": "wesjet",
  "description": "Wesjet is a JavaScript library to transform and validate content for building user interfaces.",
  "version": "0.0.25",
  "license": "MIT",
  "engines": {
    "node": ">=14.18"
  },
  "homepage": "https://wesbitty.org/product/wesjet",
  "bugs": "https://github.com/wesbitty/wesjet/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbitty/wesjet.git",
    "directory": "packages/wesjet"
  },
  "keywords": [
    "wesjet"
  ],
  "files": [
    "./bin/install-native.cjs",
    "./dist/**/*.{cjs,js,ts,map}",
    "./dist/!(__test__)/**/*.{js,ts,map}",
    "index.d.ts",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "bin": "./bin/install-native.cjs",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "import": "./dist/client/index.js"
    },
    "./core": {
      "import": "./dist/core.js"
    },
    "./function.js": {
      "import": "./dist/function.js/index.js"
    },
    "./function.js/node": {
      "import": "./dist/function.js/node/index.js"
    },
    "./preset": {
      "import": "./dist/preset/index.js"
    },
    "./preset/schema": {
      "import": "./dist/preset/schema/index.js"
    },
    "./sanity": {
      "import": "./dist/shared/sanity/index.js"
    },
    "./contentful": {
      "import": "./dist/shared/contentful/index.js"
    },
    "./stackbit": {
      "import": "./dist/shared/stackbit/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "./package.json"
      ],
      "client": [
        "./dist/client"
      ],
      "core": [
        "./dist/core"
      ],
      "function.js": [
        "./dist/function.js"
      ],
      "function.js/node": [
        "./dist/function.js/node"
      ],
      "preset": [
        "./dist/preset"
      ],
      "preset/schema": [
        "./dist/preset/schema"
      ],
      "contentful": [
        "./dist/shared/contentful"
      ],
      "stackbit": [
        "./dist/shared/stackbit"
      ],
      "sanity": [
        "./dist/shared/sanity"
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./bin/install-native.cjs postinstall"
  },
  "peerDependencies": {
    "@stackbit/sdk": "0.3.x",
    "next": "^12 || ^13",
    "react": "*",
    "react-dom": "*"
  },
  "peerDependenciesMeta": {
    "@stackbit/sdk": {
      "optional": true
    }
  },
  "dependencies": {
    "@sanity/client": "^2.10.5",
    "@sanity/core": "^2.10.5",
    "@sanity/image-url": "^0.140.22",
    "@sanity/schema": "^2.10.0",
    "@stackbit/sdk": "~0.3.25",
    "@wesjet/core": "^0.0.25",
    "@wesjet/preset": "^0.0.25",
    "@wesjet/function.js": "^0.0.25",
    "clipanion": "^3.2.0-rc.14",
    "contentful-management": "^7.22.4",
    "rxjs": "^7.1.0",
    "typanion": "^3.12.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "next": "^13.1.2",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "0.29.7"
  }
}
