{
  "name": "wesjet",
  "version": "0.0.20",
  "license": "MIT",
  "author": "Wesbitty, Inc",
  "homepage": "https://github.com/wesbitty/wesjet",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesbitty/wesjet"
  },
  "bin": "./bin/install-cmd.cjs",
  "type": "module",
  "engines": {
    "node": ">=14.18"
  },
  "exports": {
    "./package.json": "./package.json",
    "./maker": {
      "import": "./dist/maker.js"
    },
    "./client": {
      "import": "./dist/client/index.js"
    },
    "./utils": {
      "import": "./dist/utils/index.js"
    },
    "./utils/node": {
      "import": "./dist/utils/node/index.js"
    },
    "./core": {
      "import": "./dist/core.js"
    },
    "./next": {
      "import": "./dist/next/index.js",
      "require": "./dist/next/_jsx_.cjs"
    }
  },
  "_typesVersions": "Not needed anymore once this issue is closed https://github.com/microsoft/TypeScript/issues/33079",
  "typesVersions": {
    "*": {
      "package.json": [
        "./package.json"
      ],
      "maker": [
        "./dist/maker"
      ],
      "client": [
        "./dist/client"
      ],
      "utils": [
        "./dist/utils"
      ],
      "utils/node": [
        "./dist/utils/node"
      ],
      "core": [
        "./dist/core"
      ],
      "next": [
        "./dist/next"
      ]
    }
  },
  "files": [
    "bin",
    "dist",
    "core.js",
    "core.d.ts",
    "maker.js",
    "maker.d.ts",
    "client.js",
    "client.d.ts",
    "index.d.ts",
    "next.d.ts",
    "next.js",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "postinstall": "node ./bin/install-cmd.cjs postinstall",
    "prepack": "sed '/^import /d' dist/next/_jsx_.js > dist/next/_jsx_.cjs; cp src/next/lib/jsx-runtime.cjs dist/next/lib/jsx-runtime.cjs",
    "_bundle-cli": "esbuild ./dist/cli/index.js --bundle --platform=node --format=esm --external:esbuild --external:@esbuild-plugins/node-resolve --outfile=dist/cli/bundle.js --main-fields=module,main --metafile=meta.json --banner:js=\"import { createRequire as topLevelCreateRequire } from 'module'; const require = topLevelCreateRequire(import.meta.url); const __dirname = '__SET_BY_ESBUILD__';\""
  },
  "peerDependencies": {
    "@stackbit/sdk": "^0.2.39",
    "next": "^12 || ^13",
    "react": "*",
    "react-dom": "*"
  },
  "peerDependenciesMeta": {
    "@stackbit/sdk": {
      "optional": true
    }
  },
  "dependencies": {
    "@sanity/client": "^2.10.5",
    "@sanity/core": "^2.10.5",
    "@sanity/image-url": "^0.140.22",
    "@sanity/schema": "^2.10.0",
    "@stackbit/sdk": "^0.2.39",
    "@wesjet/core": "^0.0.20",
    "@wesjet/maker": "^0.0.20",
    "@wesjet/utils": "^0.0.20",
    "clipanion": "^3.2.0-rc.14",
    "contentful-management": "^7.22.4",
    "rxjs": "^7.1.0",
    "typanion": "^3.12.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "next": "^13.1.2",
    "typescript": "^4.9.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.1"
  }
}
